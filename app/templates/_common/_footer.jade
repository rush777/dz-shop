footer.b-footer
    .b-footer-container
        .b-logo-footer
            +logo("/img/logo-footer.png","ЭпплShop")
        +link-list("b-footer-menu", "О магазине", {
            "О нас":"#",
            "Наши цены":"#",
            "Наши скидки":"#",
            "Акции":"#"
        })
        +link-list("b-footer-menu", "Каталог", {
            "Смартфоны":"#",
            "Планшеты":"#",
            "Компьютеры":"#",
            "Аксессуары":"#"
        })
        +link-list("b-footer-menu", "Контакты", {
            "Телефоны":"#",
            "Адреса":"#",
            "Карта проезда":"#",
            "Обратная связь":"#"
        })
        .b-mobile-and-socials
            include ../_includes/_mobile-stores
            include ../_includes/_social
            
        <!-- build:js js/vendor.js --> 
        //bower:js
        script(src='/bower/modernizr/modernizr.js')
        script(src='/bower/jquery/dist/jquery.js')
        script(src='/bower/jscrollpane/script/jquery.mousewheel.js')
        script(src='/bower/jscrollpane/script/mwheelIntent.js')
        script(src='/bower/jscrollpane/script/jquery.jscrollpane.js')
        script(src='/bower/jscrollpane/script/jquery.jscrollpane.min.js')
        //endbower  
        <!-- endbuild -->
        
        <!-- build:js js/main.js --> 
        script(src="js/main.js")
        <!-- endbuild -->